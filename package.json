    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.0",
    "uniqid": "^5.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
